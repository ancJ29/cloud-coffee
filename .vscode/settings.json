{
  "files.exclude": {
    "**/node_modules": true
  },
  "cSpell.ignorePaths": ["./src/configs/i18n/vi.ts"],
  "cSpell.words": [
    "checkin",
    "ctkper",
    "exceljs",
    "mantine",
    "Qrcode",
    "saigontek",
    "tabler",
    "timesheet",
    "VITE"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.addMissingImports": "explicit",
    "source.removeUnusedImports": "explicit"
  }
}
